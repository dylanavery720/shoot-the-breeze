{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "arrow-body-style": [0],
    "quotes": [2, "single"],
    "eol-last": [0],
    "import/no-extraneous-dependencies": [0],
    "no-mixed-requires": [0],
    "import/no-unresolved": [0],
    "no-param-reassign": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [0],
    "semi": [0]
  }
}
